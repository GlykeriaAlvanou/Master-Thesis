@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn> .
@prefix mso: <http://example.com/owl/machineOntology/> .

# Description of this ontology
# ----------------------------

mso:
	 rdf:type owl:Ontology;
   	 dct:title "Machine Sensor Ontology"@en;
     rdfs:comment "Machine Ontology";
     dct:creator "Glykeria Alvanou" ;
     dct:created "2017-06-05"^^xsd:date;
     dct:modified "2017-06-15"^^xsd:date.


##### CLASSES #####

### Structural Elements ###
mso:StructuralElements 
		rdf:type owl:Class;
		rdfs:comment "Physical and logical parts and sub-parts of a device".

### Devices ###
mso:Devices 
		rdf:type owl:Class;
        rdfs:subClassOf mso:StructuralElements;
        [rdf:type         owl:Restriction ; ###has 1 or more Device
         owl:onProperty   mso:hasDevice;
         owl:mincardinality  "1"^^xsd:nonNegativeInteger ;
 ] .


### Device  has 1 Components ###
mso:Device 
		rdf:type owl:Class;
		rdfs:subClassOf mso:Devices;
		rdfs:comment "Represents a single piece of equipment";
		 [rdf:type         owl:Restriction ; 
         owl:onProperty   mso:hasuuid;
         owl:cardinality  "1"^^xsd:nonNegativeInteger ;
 ] .

### Components has 1 or more Component ###
mso:Components 
			rdf:type owl:Class ;
			rdfs:comment "Provides structure for the physical and logical sub-elements of a device";
           # rdfs:subClassOf :Device;
           [rdf:type         owl:Restriction ; ### has 1 or more Component
            owl:onProperty   mso:hasComponent;
            owl:mincardinality  "1"^^xsd:nonNegativeInteger ;
 ] .

### Component has 1 or more ###
mso:Component 
			rdf:type owl:Class;
			[rdf:type         owl:Restriction ; 
             owl:onProperty   mso:hasid;
             owl:cardinality  "1"^^xsd:nonNegativeInteger ;
 ] .

### Subcomponent has 1 or more ###
mso:Subcomponent 
			 rdf:type owl:Class;
			# rdfs:subClassOf mso:Component;
		     rdfs:comment "Physical" .

### Data elements ###
mso:DataElements 
			rdf:type owl:Class,
			rdfs:comment "Data that can be collected from a device".

### DataItems has 1 or more DataItem: cardinality ###
mso:DataItems 
			rdf:type owl:Class ;
			rdfs:comment "Provides the structure for organising data from a device.";
			rdfs:subClassOf mso:DataElements;
			[rdf:type         owl:Restriction ; ### has 1 or more Component
            owl:onProperty   mso:hasDataItem;
            owl:mincardinality  "1"^^xsd:nonNegativeInteger ;
 ] .

### DataItem ###
mso:DataItem 
		 rdf:type owl:Class;
		 rdfs:comment "Provides a detailed description for each piece of data the is collected from a device.";
		 rdfs:subClassOf mso:DataItems,
		 rdfs:comment .
##DataItem types
#temperature, pressure, velocity etc

##Component types
mso:axes rdf:type mso:Component.
mso:controller rdf:type mso:Component.
mso:systems rdf:type mso:Component.
mso:door rdf:type mso:Component.
mso:sensor rdf:type mso:Component.
mso:stock rdf:type mso:Component.
mso:interfaces rdf:type mso:Component.
#### Properties ####

#### Object Properties ####

mso:hasDevice 	
			rdf:type 	owl:ObjectProperty;
		    rdfs:domain 	mso:Devices;
		    rdfs:range 		mso:Device.

mso:hasComponent 	
				rdf:type 	owl:ObjectProperty;
		      	rdfs:domain 	mso:Components;
		      	rdfs:range 	mso:Component.
mso:hasDataItem 	
				rdf:type 	owl:ObjectProperty;
		      	rdfs:domain 	mso:DataItems;
		      	rdfs:range 	mso:DataItem.

#mso:hasSubElement 	
				#rdf:type 	owl:ObjectProperty;
				
			#rdfs:domain 	mso:Device,
								#mso:Component.


#### Datatype Properties ####

mso:hasuuid	
			rdf:type 	owl:ObjectProperty;
			rdfs:domain 	mso:Device;
			rdfs:range 		 rdfs:Literal.

mso:hasname	
			rdf:type 	owl:ObjectProperty;
			rdfs:domain 	mso:Device;
			rdfs:range 		 rdfs:Literal.

mso:hasid	
			rdf:type 	owl:ObjectProperty;
			rdfs:domain 	mso:Device, mso:Component, mso:DataItem ;
			rdfs:range 		 rdfs:Literal.

mso:hastype 	
				rdf:type 	owl:ObjectProperty;
		      	rdfs:domain 	mso:DataItem;
		      	rdfs:range 	 rdfs:Literal.
			
mso:hascategory	
				rdf:type 	owl:ObjectProperty;
		      	rdfs:domain 	mso:DataItem;
		      	rdfs:range 	 rdfs:Literal.

